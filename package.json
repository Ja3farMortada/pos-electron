{
	"name": "morexa",
	"version": "1.19.7",
	"description": "Morexa",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"test": "rm -rf dist && electron-builder --x64 --win",
		"build": "rm -rf dist && electron-builder --x64 --win --publish always"
	},
	"build": {
		"appId": "com.binariez.Ja3farMortada.pos-electron",
		"productName": "Morexa",
		"artifactName": "${name}-${version}.${ext}",
		"target": "NSIS",
		"win": {
			"target": "nsis"
		},
		"publish": [
			{
				"provider": "github",
				"owner": "Ja3farMortada",
				"repo": "pos-electron",
				"releaseType": "release"
			}
		],
		"extraResources": [
			"./server"
		],
		"npmRebuild": "false",
		"directories": {
			"buildResources": "resources"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ja3farMortada/pos-electron.git"
	},
	"author": "Binariez LLC",
	"license": "ISC",
	"dependencies": {
		"angular": "^1.8.3",
		"bootstrap": "^5.3.3",
		"bootstrap-icons": "^1.11.3",
		"electron-context-menu": "^3.6.1",
		"electron-log": "^4.4.8",
		"electron-updater": "^6.1.1",
		"exceljs": "^4.4.0",
		"jsbarcode": "^3.12.1",
		"moment": "^2.29.4",
		"mysql2": "^3.6.0",
		"mysqldump": "^3.2.0"
	},
	"devDependencies": {
		"electron": "^30.3.1",
		"electron-builder": "^24.6.3"
	}
}