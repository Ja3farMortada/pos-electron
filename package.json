{
  "name": "morexa",
  "version": "1.23.2",
  "description": "Morexa",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "rm -rf dist && electron-builder --x64 --win --publish always"
  },
  "build": {
    "appId": "com.binariez.Ja3farMortada.pos-electron",
    "productName": "Morexa",
    "artifactName": "${name}-${version}.${ext}",
    "win": {
      "target": "nsis"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Ja3farMortada",
        "repo": "pos-electron",
        "releaseType": "release"
      }
    ],
    "extraResources": [
      "./server"
    ],
    "npmRebuild": false,
    "directories": {
      "buildResources": "resources"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ja3farMortada/pos-electron.git"
  },
  "author": "Binariez LLC",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.8.3",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "electron-context-menu": "^4.1.1",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "exceljs": "^4.4.0",
    "jsbarcode": "^3.12.1",
    "moment": "^2.30.1",
    "mysql2": "^3.15.3",
    "mysqldump": "^3.2.0",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12"
  }
}
